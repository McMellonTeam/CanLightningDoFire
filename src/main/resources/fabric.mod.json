	"authors": [
		"RodoFire"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/pistonpushmod/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.louis.pistonpushmod.PistonPushMod"
		],
		"client": [
			"net.louis.pistonpushmod.PistonPushModClient"
		],
		"fabric-datagen": [
			"net.louis.pistonpushmod.PistonPushModDataGenerator"
		]
	},
	"mixins": [
		"pistonpushmod.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14.21",
